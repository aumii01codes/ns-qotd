#include <bits/stdc++.h> // header file includes every Standard library
using namespace std;

int main() {
    int N;
    cin >> N;
    int A[N];
    for (int i = 0; i < N; i++) {
        cin >> A[i];
    }
    sort(A, A + N); // sort the array in ascending order
    int missing = 0;
    for (int i = 0; i < N; i++) {
        if (A[i] == missing) {
            missing++;
        } else if (A[i] > missing) {
            break;
        }
    }
    cout << missing << endl;

    return 0;
}
