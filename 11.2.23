#include <bits/stdc++.h> // header file includes every Standard library
using namespace std;

int main() {
    long long N;
    int K;
    cin >> N >> K;
    for (int i = 0; i < K; i++) {
        if (N % 200 == 0) {
            N /= 200;
        } else {
            N = N * 1000 + 200;
        }
    }
    cout << N << endl;
    return 0;
}
