#include <bits/stdc++.h> // header file includes every Standard library
using namespace std;
int main() {
	int n, i;
    int sum=0;
    cin>>n;
    for(i=1; i<=n; i++) {
        if(n%i == 0) {
            sum += i;
        }
    }
    sum -= n;
    if (sum==n) cout<<n*n;
    else cout<<"-1";
    return 0;
}
