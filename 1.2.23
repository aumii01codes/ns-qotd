#include <bits/stdc++.h>
using namespace std;

string reverse_substring(int l, int r, string s) {
    reverse(s.begin() + l - 1, s.begin() + r);
    return s;
}

int main() {
    int l, r;
    cin >> l >> r;
    string s;
    cin >> s;
    cout << reverse_substring(l, r, s) << endl;
    return 0;
}
