#include <bits/stdc++.h>
using namespace std;
int main() {
    string s;
    cin >> s;
    // Shift the string one position to the right
    for (int i = s.length() - 1; i >= 1; i--) {
        s[i] = s[i-1];
    }  
    s[0] = '0'; // The leftmost position should be empty
    cout << s << endl;
    return 0;
}
